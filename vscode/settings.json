{
  "atomKeymap.promptV3Features": true,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.formatOnPaste": true,
  "files.autoSave": "afterDelay",
  "cSpell.userWords": [
    "Deserialize",
    "Dropdown",
    "Guid",
    "Idam",
    "Maccora",
    "Payrun",
    "Postgre",
    "backend",
    "myob",
    "validators"
  ],
  "editor.scrollBeyondLastLine": false,
  "explorer.openEditors.visible": 4,
  "git.autofetch": true,
  "gitlens.keymap": "chorded",
  "editor.formatOnSave": true,
  "gitProjectManager.baseProjectsFolders": ["$HOME/dev"],
  "cSpell.enabledLanguageIds": [
    "c",
    "cpp",
    "csharp",
    "css",
    "go",
    "handlebars",
    "html",
    "jade",
    "javascript",
    "javascriptreact",
    "json",
    "latex",
    "less",
    "markdown",
    "php",
    "plaintext",
    "python",
    "pub",
    "puml",
    "restructuredtext",
    "scss",
    "text",
    "yml"
  ],
  "gitlens.advanced.messages": {
    "suppressCommitHasNoPreviousCommitWarning": false,
    "suppressCommitNotFoundWarning": false,
    "suppressFileNotUnderSourceControlWarning": false,
    "suppressGitVersionWarning": false,
    "suppressLineUncommittedWarning": false,
    "suppressNoRepositoryWarning": false,
    "suppressResultsExplorerNotice": false,
    "suppressShowKeyBindingsNotice": true
  },
  "markdown.extension.orderedList.marker": "one",
  "[markdown]": {
    "editor.wordWrap": "on",
    "editor.formatOnSave": false,
    "editor.minimap.enabled": false,
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  "cSpell.language": "en,en-GB",
  "files.autoSaveDelay": 3000,
  "plantuml.exportSubFolder": false,
  "editor.cursorBlinking": "expand",
  "python.pythonPath": "python3",
  "extensions.ignoreRecommendations": false,
  "prettier.singleQuote": true,
  "javascript.implicitProjectConfig.experimentalDecorators": true,
  "auto-close-tag.activationOnLanguage": [
    "xml",
    "php",
    "blade",
    "ejs",
    "jinja",
    "javascript",
    "javascriptreact",
    "typescriptreact",
    "plaintext",
    "markdown",
    "vue",
    "liquid",
    "erb",
    "lang-cfml",
    "cfml",
    "HTML (Eex)"
  ],
  "auto-close-tag.excludedTags": [
    "area",
    "base",
    "br",
    "col",
    "command",
    "embed",
    "hr",
    "img",
    "input",
    "keygen",
    "link",
    "meta",
    "param",
    "source",
    "string",
    "track",
    "wbr"
  ],
  "window.zoomLevel": 0,
  "gitlens.historyExplorer.enabled": true,
  "editor.tokenColorCustomizations": {
    "[Material Theme Darker]": {
      "textMateRules": [
        {
          "scope": "meta.attribute",
          "settings": {
            "foreground": "#82AAFF"
          }
        }
      ]
    }
  },
  "files.trimTrailingWhitespace": true,
  "workbench.colorCustomizations": {
    // "statusBar.background": "#564558",
    "terminal.ansiYellow": "#ffa727",
    "terminal.ansiBlue": "#ae80fe"
    // "activityBar.background": "#282A2E",
    // "sideBar.background": "#282A2E"
  },
  "materialTheme.accent": "Purple",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "terminal.integrated.fontFamily": "'DroidSansMono Nerd Font Mono'",
  "breadcrumbs.enabled": true,
  "rust.all_features": true,
  "rust.clippy_preference": "on",
  "zenMode.centerLayout": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "azure.resourceFilter": [
    "713b4827-e931-4abf-acf5-de26dc013f5c/4ecc4eb7-047d-4b57-9feb-56732ae17183"
  ],
  "gitlens.views.fileHistory.enabled": true,
  "gitlens.views.lineHistory.enabled": true,
  "tslint.autoFixOnSave": true,
  "editor.fontFamily": "Fira Code, Menlo, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.minimap.enabled": false,
  "workbench.sideBar.location": "right",
  "zenMode.fullScreen": false,
  "window.title": "${rootName}${separator}${activeEditorShort}",
  "git.rebaseWhenSync": true,
  "files.exclude": {
    "**/.history": true,
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "editor.fontSize": 13,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "tslint.configFile": "tslint.json",
  "eslint.validate": [
    {
      "language": "typescript",
      "autoFix": true
    },
    {
      "language": "typescriptreact",
      "autoFix": true
    }
  ],
  "tslint.enable": false,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "eslint.alwaysShowStatus": true,
  "eslint.autoFixOnSave": true,
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "workbench.iconTheme": "vscode-icons",
  "ruby.format": "rubocop",
  "editor.formatOnSaveTimeout": 1500,
  "terminal.integrated.shell.osx": "fish",
  "plantuml.server": "https://plantuml.corp.amazon.com/",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "rewrap.wrappingColumn": 120,
  "diffEditor.ignoreTrimWhitespace": false,
  "vsnotes.defaultNotePath": "/Users/dmaccora/Notes",
  "diffEditor.renderSideBySide": true,
  "todo-tree.tree.showScanModeButton": false,
  "workbench.colorTheme": "Ayu Mirage",
  "java.semanticHighlighting.enabled": true,
  "vim.smartRelativeLine": true,
  "editor.lineNumbers": "relative",
}
