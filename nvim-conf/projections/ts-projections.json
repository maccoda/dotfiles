{
    "src/*.ts": {
        "alternate": "src/{}.test.ts",
        "type": "source"
    },
    "src/*.tsx": {
        "alternate": "src/{}.test.tsx",
        "type": "source"
    },
    "src/*.test.tsx": {
        "alternate": "src/{}.tsx",
        "type": "test"
    },
    "src/*.test.ts": {
        "alternate": "src/{}.ts",
        "type": "test"
    }
}
