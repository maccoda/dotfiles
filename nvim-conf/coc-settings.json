{
    "suggest.noselect": false,
    "coc.preferences.formatOnSaveFiletypes": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact",
        "rust",
        "markdown"
      ],
      "tsserver.formatOnType": true,
      "coc.preferences.formatOnInsertLeave": true,
      "languageserver": {
          "kotlin": {
              "command": "~/tools/kotlin-lsp/server/bin/kotlin-language-server",
              "filetypes": ["kotlin"]
          }
      },
}
