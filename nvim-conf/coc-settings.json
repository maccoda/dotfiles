{
    "suggest.noselect": false,
    "coc.preferences.formatOnSaveFiletypes": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact",
        "rust"
      ],
      "tsserver.formatOnType": true,
      "coc.preferences.formatOnType": true,
      "languageserver": {
          "kotlin": {
              "command": "~/tools/kotlin-lsp/server/bin/kotlin-language-server",
              "filetypes": ["kotlin"]
          }
      },
      "diagnostic.displayByAle": true
}
