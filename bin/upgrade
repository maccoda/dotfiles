#!/usr/local/bin/bash
set -x
case "$(uname -s)" in

   Darwin)
     brew cask upgrade; brew upgrade; brew cleanup
    ;;

   Linux)
        sudo apt-get update
        sudo apt-get upgrade
        sudo apt-get autoremove
    ;;

   *)
     echo 'Other OS'
     ;;
esac

fish -c "omf update"

rustup update

cd /Users/dmaccora/.local/share/omf/themes/batman
git pull
